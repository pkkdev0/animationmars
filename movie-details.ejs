    }

    .navbar {
      position: fixed;
      top: 0;
      right: -300px;
      background: var(--eerie-black);
      width: 100%;
      max-width: 300px;
      height: 100%;
      box-shadow: -1px 0 3px hsl(0, 0%, 0%, 0.2);
      transition: 0.15s ease-in;
      visibility: hidden;
      z-index: 3;
    }

    .navbar.active {
      right: 0;
