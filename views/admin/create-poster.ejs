<script>
  document.querySelectorAll('.select-field').forEach(function(selectField) {
      selectField.addEventListener('click', function() {
          // Find the sibling .list of the clicked .select-field
          var correspondingList = this.nextElementSibling;

          // Toggle the 'show' class for the corresponding .list
          if(correspondingList) {
              correspondingList.classList.toggle('show');
          }

          // Find the child .down-arrow of the clicked .select-field to toggle its class
          var arrow = this.querySelector('.down-arrow');
          if(arrow) {
              arrow.classList.toggle('rotate180');
          }
      });
  });
  // Since the id should be unique, change it to a class for multiple use
  document.querySelectorAll('.input-selector').forEach(function(inputSelector) {
      inputSelector.addEventListener('keyup', function() {
          var searchVal = this.value.toLowerCase();
          // Find the corresponding .list for the input being used for search
          var list = this.closest('.multi-selector').querySelector('.list');

          // Filter the items in the .list based on the search value
          if(list) {
              var tasks = list.querySelectorAll('.task');
              tasks.forEach(function(task) {
                  var text = task.textContent || task.innerText;
                  if (text.toLowerCase().indexOf(searchVal) > -1) {
                      task.style.display = "";
                  } else {
                      task.style.display = "none";
                  }
              });
          }
      });
  });
</script>







                    

           


